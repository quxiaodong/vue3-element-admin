{
  "name": "miz-vue-ele",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "openapi": "npx swagger-typescript-api -p http://localhost:3000/swagger/json -o ./src/openapi/__generated__ -n index.ts --clean-output --no-client",
    "prepare": "husky",
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "build:dev": "vue-tsc -b && vite build --mode development",
    "dev:mock": "vite --mode mock",
    "build:mock": "vue-tsc -b && vite build --mode mock",
    "preview": "vite preview"
  },
  "lint-staged": {
    "src/**/*.{js,ts,jsx,tsx,vue}": [
      "eslint --fix"
    ],
    "src/**/*": [
      "prettier --write --ignore-unknown"
    ]
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "@vueuse/core": "^11.1.0",
    "axios": "^1.7.7",
    "color": "^4.2.3",
    "element-plus": "^2.8.4",
    "mockjs": "^1.1.0",
    "nprogress": "^0.2.0",
    "pinia": "^2.2.4",
    "qs": "^6.13.1",
    "sortablejs": "^1.15.3",
    "vue": "^3.5.12",
    "vue-i18n": "^10.0.4",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@eslint/js": "^9.11.1",
    "@types/color": "^4.2.0",
    "@types/eslint__js": "^8.42.3",
    "@types/mockjs": "^1.0.10",
    "@types/nprogress": "^0.2.3",
    "@types/qs": "^6.9.17",
    "@types/sortablejs": "^1.15.8",
    "@vitejs/plugin-vue": "^5.1.2",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-vue": "^9.28.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "swagger-typescript-api": "^13.0.22",
    "tailwindcss": "^3.4.13",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.8.0",
    "vite": "^5.4.1",
    "vue-tsc": "^2.1.8"
  },
  "license": "MIT"
}
